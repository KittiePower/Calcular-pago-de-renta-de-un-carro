Int main ()
{
    //Declaraci√≥n de variables 
    float renta, Kilometro, Establecido;
    
    
    //Entrada de datos por teclado y procedimientos 
    cout<<" Escribe el valor del Kilometro " ;
    cin>>Kilometro;
    cout<<" Escribe el valor Establecido " ;
